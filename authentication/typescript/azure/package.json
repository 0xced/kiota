  "author": "Microsoft",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/microsoft/kiota/issues"
  },
  "homepage": "https://github.com/microsoft/kiota#readme",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "tslib": "^2.3.1",
    "@azure/core-auth": "^1.3.2",
    "@microsoft/kiota-abstractions": "^1.0.27"
  },
  "devDependencies": {
    "@types/node": "^17.0.14",
    "typescript": "^4.5.5",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/sinon": "^10.0.7",
    "chai": "^4.2.0",
    "esm": "^3.2.25",
    "mocha": "^6.2.3",
    "sinon": "^12.0.1",
    "@azure/identity": "^2.0.1"
  }
